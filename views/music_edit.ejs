<h1>Edit song details</h1>

<form action="/music/<%= music.id %>?_method=put" method="post">
    <label for="">Song Name</label>
    <input type="text" name="title" value="<%= music.title %>"><br>

    <label for="">Artist</label>
    <input type="text" name="artist" value="<%= music.artist %>"><br>

    <label for="">Image URL</label>
    <input type="text" name="image_url" value="<%= music.image_url %>"><br>

    <label for="">Year</label>
    <input type="text" name="year" value="<%= music.year %>"><br>

    <label for="">Genre</label>
    <input type="text" name="genre" value="<%= music.genre %>"><br><br>

    <button>Update</button>
</form>